    "stemmer = SnowballStemmer(\"english\")\n",
    "\n",
    "def stem(text):\n",
    "    words = text.split()\n",
    "    stems = [stemmer.stem(t) for t in words]\n",
    "    return \" \".join(stems)"
   ]
